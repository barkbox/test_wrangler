<div class="page-header">
  <h2>Experiments</h2> 
</div>
<section class="form">
<table>
  <thead>
    <tr>
      <td>Name</td>
      <td>State</td>
      <td>Cohorts</td>
    </tr>
  </thead>
  <tbody>
  {{~ it :experiment}}
    <tr>
      <td><a href="/test_wrangler/dashboard/experiments/{{= experiment.id}}">{{= experiment.id }}</a></td><td>{{= experiment.attributes.state }}</td><td>{{= experiment.attributes.cohorts.join(', ')}}
    </tr>
  {{~}}
  </tbody>
</table>
</section>